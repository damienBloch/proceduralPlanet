    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from scipy.special import gamma "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "metadata": {
    "ExecuteTime": {
     "end_time": "2020-09-26T18:19:14.926945Z",
